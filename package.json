{
  "name": "google-books-react-app",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "setocourtney",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "engine.io": "^3.4.1",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.9.14",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "uws": "^10.148.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/setocourtney/google-books-react-app.git"
  },
  "keywords": [
    "google",
    "books",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/setocourtney/google-books-react-app/issues"
  },
  "homepage": "https://github.com/setocourtney/google-books-react-app#readme"
}
